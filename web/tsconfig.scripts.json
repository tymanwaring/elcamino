{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./scripts/dist",
    "rootDir": "./scripts",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2020",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": false
  },
  "include": [
    "scripts/tee-bot.ts"
  ],
  "exclude": [
    "node_modules",
    ".next"
  ]
}

